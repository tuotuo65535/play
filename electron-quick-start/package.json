  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "packager": "electron-packager . HelloWorld --out ./OutApp"
  },
  "keywords": [
    "HelloWorld"
